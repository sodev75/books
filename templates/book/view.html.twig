{% extends 'base.html.twig' %}

{% block title %}{{ book.title }}{% endblock %}

  {% block body %}
      <header>
          <h1>{{ book.title }}</h1>
      </header>
      <section class="book">
          {{ book.author }}
          {{ book.publisher }}
          {{ book.subject }}
          {{ book.mainCategory }}
          {{ book.proposedBy }}
          {{ book.pageCount }}

      </section>

{% endblock %}
